# .env example file - Copy this to .env and fill in your actual values

PORT=5000
NODE_ENV=development

# MongoDB Atlas Connection String
# Format: mongodb+srv://<username>:<password>@<cluster-url>/?retryWrites=true&w=majority
# Note: If password contains special characters, they should be URL encoded
# For example: @ becomes %40, # becomes %23, : becomes %3A, etc.
MONGODB_URI=mongodb+srv://username:password@cluster0.xxxxx.mongodb.net/?retryWrites=true&w=majority

# JWT Secret - Use a strong random string in production
JWT_SECRET=your_very_long_random_secret_key_here_change_in_production

# Frontend URL
FRONTEND_URL=http://localhost:3000
